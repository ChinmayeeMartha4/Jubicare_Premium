<?xml version="1.0" encoding="utf-8"?><!--<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="@dimen/_5dp">-->

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/login_bg"
    android:orientation="vertical"
    tools:context=".activity.CommonProfile">

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/margin10"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_20dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="@dimen/_3dp">

                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/iv_profile_pic"
                        android:layout_width="@dimen/margin100"
                        android:layout_height="@dimen/margin100"
                        android:layout_gravity="center"
                        android:src="@drawable/male_icon"
                        android:visibility="visible"
                        app:civ_border_color="@color/colorGrayLight"
                        app:civ_border_width="@dimen/margin2" />
                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin10"
                    android:layout_marginBottom="3dp"
                    android:fontFamily="@font/robotoregular"
                    android:text="@string/patientName"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/text_16"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin5"
                    android:orientation="vertical"
                    android:textColor="@android:color/black">

                    <TextView
                        android:id="@+id/tv_patient_name"
                        style="@style/StyleTheme"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@android:color/transparent"
                        android:fontFamily="@font/robotoregular"
                        android:text="" />

                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin10"
                    android:layout_marginBottom="3dp"
                    android:fontFamily="@font/robotoregular"
                    android:text="@string/Gender"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/text_16"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5dp"
                    android:orientation="vertical"
                    android:textColor="@android:color/black">

                    <TextView
                        android:id="@+id/tv_gender"
                        style="@style/StyleTheme"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@android:color/transparent"
                        android:fontFamily="@font/robotoregular"
                        android:text="" />

                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin10"
                    android:layout_marginBottom="3dp"
                    android:fontFamily="@font/robotoregular"
                    android:text="@string/DateofBirth"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/text_16"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5dp"
                    android:orientation="vertical"
                    android:textColor="@android:color/black">

                    <TextView
                        android:id="@+id/tv_dob"
                        style="@style/StyleTheme"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@android:color/transparent"
                        android:fontFamily="@font/robotoregular"
                        android:text="" />

                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin10"
                    android:layout_marginBottom="3dp"
                    android:fontFamily="@font/robotoregular"
                    android:text="@string/age"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/text_16"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5dp"
                    android:orientation="vertical"
                    android:textColor="@android:color/black">

                    <TextView
                        android:id="@+id/tv_age"
                        style="@style/StyleTheme"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@android:color/transparent"
                        android:fontFamily="@font/robotoregular"
                        android:text="" />

                </LinearLayout>
                <!--height, weight and blood group-->
                <TextView
                    android:id="@+id/tv_height_cms"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin10"
                    android:layout_marginBottom="3dp"
                    android:fontFamily="@font/robotoregular"
                    android:text="@string/height"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/text_16"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5dp"
                    android:orientation="vertical"
                    android:textColor="@android:color/black">

                    <TextView
                        android:id="@+id/tv_height"
                        style="@style/StyleTheme"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@android:color/transparent"
                        android:fontFamily="@font/robotoregular"
                        android:text="" />

                </LinearLayout>

                <TextView
                    android:id="@+id/tv_weight_kgs"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin10"
                    android:layout_marginBottom="3dp"
                    android:fontFamily="@font/robotoregular"
                    android:text="@string/weight"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/text_16"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5dp"
                    android:orientation="vertical"
                    android:textColor="@android:color/black">

                    <TextView
                        android:id="@+id/tv_weight"
                        style="@style/StyleTheme"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@android:color/transparent"
                        android:fontFamily="@font/robotoregular"
                        android:text="" />

                </LinearLayout>



                <TextView
                    android:id="@+id/tv_bg"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin10"
                    android:layout_marginBottom="3dp"
                    android:fontFamily="@font/robotoregular"
                    android:text="@string/blood"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/text_16"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5dp"
                    android:orientation="vertical"
                    android:textColor="@android:color/black">

                    <TextView
                        android:id="@+id/tv_blood_group"
                        style="@style/StyleTheme"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@android:color/transparent"
                        android:fontFamily="@font/robotoregular"
                        android:text="" />

                </LinearLayout>

                <TextView
                    android:id="@+id/tv_caste"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin10"
                    android:layout_marginBottom="3dp"
                    android:fontFamily="@font/robotoregular"
                    android:text="@string/caste"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/text_16"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5dp"
                    android:orientation="vertical"
                    android:textColor="@android:color/black">

                    <TextView
                        android:id="@+id/tv_caste_text"
                        style="@style/StyleTheme"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@android:color/transparent"
                        android:fontFamily="@font/robotoregular"
                        android:text="" />

                </LinearLayout>

                <TextView
                    android:id="@+id/tv_disability"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin10"
                    android:layout_marginBottom="3dp"
                    android:fontFamily="@font/robotoregular"
                    android:text="People with Disability (PwDs)"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/text_16"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5dp"
                    android:orientation="vertical"
                    android:textColor="@android:color/black">

                    <TextView
                        android:id="@+id/tv_disability_text"
                        style="@style/StyleTheme"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@android:color/transparent"
                        android:fontFamily="@font/robotoregular"
                        android:text="" />

                </LinearLayout>


                <TextView
                    android:id="@+id/tv_adhar_no"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin10"
                    android:layout_marginBottom="3dp"
                    android:fontFamily="@font/robotoregular"
                    android:text="@string/aadhar"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/text_16"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5dp"
                    android:orientation="vertical"
                    android:textColor="@android:color/black">

                    <TextView
                        android:id="@+id/tv_aadhar_no"
                        style="@style/StyleTheme"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@android:color/transparent"
                        android:fontFamily="@font/robotoregular"
                        android:text="" />

                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin10"
                    android:layout_marginBottom="3dp"
                    android:fontFamily="@font/robotoregular"
                    android:text="@string/PatientContact"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/text_16"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5dp"
                    android:orientation="vertical"
                    android:textColor="@android:color/black">

                    <TextView
                        android:id="@+id/tv_contact_no"
                        style="@style/StyleTheme"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@android:color/transparent"
                        android:fontFamily="@font/robotoregular"
                        android:text="" />

                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin10"
                    android:layout_marginBottom="3dp"
                    android:fontFamily="@font/robotoregular"
                    android:text="@string/permanent_address"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/text_16"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5dp"
                    android:orientation="horizontal"
                    android:textColor="@android:color/black">

                    <TextView
                        android:id="@+id/tv_location"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textColor="@color/colorBlack" />

                    <!--<TextView
                        android:id="@+id/tv_village"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textColor="@color/colorBlack" />

                    <TextView
                        android:id="@+id/tv_block"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2dp"
                        android:text=""
                        android:textColor="@color/colorBlack" />

                    <TextView
                        android:id="@+id/tv_district"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2dp"
                        android:text=""
                        android:textColor="@color/colorBlack" />

                    <TextView
                        android:id="@+id/tv_state"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2dp"
                        android:text=""
                        android:textColor="@color/colorBlack" />-->

                </LinearLayout>

                <TextView
                    android:id="@+id/tv_address_heading"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin10"
                    android:layout_marginBottom="3dp"
                    android:fontFamily="@font/robotoregular"
                    android:text="@string/Address"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/text_16"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5dp"
                    android:orientation="vertical"
                    android:textColor="@android:color/black">

                    <TextView
                        android:id="@+id/tv_address"
                        style="@style/StyleTheme"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@android:color/transparent"
                        android:fontFamily="@font/robotoregular"
                        android:text="" />

                </LinearLayout>

                <TextView
                    android:id="@+id/tv_code_pin"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin10"
                    android:layout_marginBottom="3dp"
                    android:fontFamily="@font/robotoregular"
                    android:text="@string/Pincode"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/text_16"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5dp"
                    android:orientation="vertical"
                    android:textColor="@android:color/black">

                    <TextView
                        android:id="@+id/tv_pin_code"
                        style="@style/StyleTheme"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@android:color/transparent"
                        android:fontFamily="@font/robotoregular"
                        android:text="" />

                </LinearLayout>

                <TextView
                    android:id="@+id/tv_emergency_contact"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin10"
                    android:layout_marginBottom="3dp"
                    android:fontFamily="@font/robotoregular"
                    android:text="@string/emergency_contact_number"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/text_16"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5dp"
                    android:orientation="vertical"
                    android:textColor="@android:color/black">

                    <TextView
                        android:id="@+id/tv_emergency_contact_no"
                        style="@style/StyleTheme"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@android:color/transparent"
                        android:fontFamily="@font/robotoregular"
                        android:text="" />

                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_15dp"
                    android:layout_marginBottom="@dimen/_3dp"
                    android:background="@drawable/shape_of_textview"
                    android:fontFamily="@font/robotoregular"
                    android:gravity="center"
                    android:padding="@dimen/_3dp"
                    android:text="@string/click_on_button_for_view_medical_information_details"
                    android:textColor="@color/menuTextColor"
                    android:textSize="@dimen/text_16"
                    android:textStyle="bold" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_1dp"
                    android:background="@color/colorBlack" />
                <!--end of general details-->

            </LinearLayout>

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scrollbars="none">

                <LinearLayout
                    android:id="@+id/ll_for_dynamic_add"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"></LinearLayout>

            </ScrollView>

        </LinearLayout>

    </ScrollView>

</LinearLayout>

    <!--</androidx.cardview.widget.CardView>-->
